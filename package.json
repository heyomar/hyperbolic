	"scripts": {
		"clean": "rm -rf dist .parcel-cache",
		"watch": "npm run clean && parcel app/index.html --no-cache --no-source-maps --open",
		"build": "npm run clean && parcel build app/index.html --dist-dir dist/"
	},
