	"license": "ISC",
	"bugs": {
		"url": "https://github.com/heyomar/share.links/issues"
	},
	"homepage": "https://github.com/heyomar/share.links#readme",
	"devDependencies": {
		"@parcel/transformer-image": "^2.0.0-rc.0",
		"autoprefixer": "^10.1.0",
		"parcel": "^2.0.0-rc.0",
		"postcss": "^8.2.2",
		"postcss-cli": "^8.3.1",
		"tailwindcss": "^2.2.15",
		"typescript": "^4.1.3"
	}
}
